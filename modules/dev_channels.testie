%info
Set the number of hardware queues for a given device

%variables
N=1
NIC=0

%init sudo=true
n=$(sudo ethtool -l ${self:$NIC:ifname} | grep -i combined | tail -n 1 | grep -oE "[0-9]+")
if [ "$n" != "$N" ] ; then
    ethtool -L ${self:$NIC:ifname} combined $N && echo "RESULT 1"
else
    echo "RESULT 1"
fi

